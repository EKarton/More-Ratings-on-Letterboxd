{
  "name": "more-ratings-on-letterboxd-chrome-extension",
  "version": "1.0.0",
  "description": "A chrome extension that adds IMDB, Rotten Tomatoes, and Metacritic ratings to Letterboxd",
  "keywords": [
    "chrome",
    "extension",
    "ratings",
    "letterboxd"
  ],
  "author": "Emilio Kartono",
  "license": "MIT",
  "engines": {
    "yarn": ">=1.22.x"
  },
  "scripts": {
    "clean": "rm -rf dist/ && rm -rf node_modules/",
    "build": "cp -r ./public/ dist/ && webpack --node-env production",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "test": "jest"
  },
  "dependencies": {
    "arrive": "^2.4.1",
    "bootstrap": "^5.1.0"
  },
  "devDependencies": {
    "autoprefixer": "10.3.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.16.4",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.1.0",
    "mini-css-extract-plugin": "^2.2.0",
    "prettier": "^2.3.2",
    "style-loader": "^3.2.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}
